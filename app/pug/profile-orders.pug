doctype html
html(lang='en')
    include modules/header/head
    body
        // main container of all the page elements
        #wrapper
            // Page Loader
            #pre-loader.loader-container
                .loader
                    img(src='images/svg/rings.svg', alt='loader')
            .w1
                // mt -header style14 start from here
                include modules/header/header
                // Main of the Page
                main#mt-main
                    section.wow.fadeInUp(data-wow-delay='0.4s')
                        .container-fluid
                            .row
                                .col-xs-12
                                    section.profile-page
                                        h2.text-center Личный кабинет
                                        .row
                                            .col-md-3.profile-nav
                                                ul
                                                    li.active
                                                        a(href="#") Мои настройки
                                                    li
                                                        a(href="#") Мои заказы
                                                    li
                                                        a(href="#") Список избранного
                                            .col-md-8.profile-data.orders-container
                                                .table-responsive
                                                    table
                                                        tr
                                                            th Код заказа
                                                            th Товар
                                                            th Дата заказа
                                                            th Сумма заказа
                                                            th Статус
                                                        tr
                                                            td 99999999
                                                            td Моторное масло SHELL HELIX HX7 10W-40 1л
                                                            td 30.07.2018
                                                            td 20 786 грн.
                                                            td В обработке
                                                        tr
                                                            td 99999999
                                                            td Моторное масло SHELL HELIX HX7 10W-40 1л
                                                            td 30.07.2018
                                                            td 20 786 грн.
                                                            td В обработке
                include modules/footer/footer 
            span#back-top.fa.fa-arrow-up
        include modules/footer/scripts